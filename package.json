{
  "name": "@4lch4/toggl-aid",
  "version": "0.2.1",
  "description": "A wrapper library for interacting with the Toggl API.",
  "main": "./dist/index.js",
  "scripts": {
    "package": "rm -rf ./dist; tsc --pretty",
    "install:clean": "rm -rf ./node_modules; rm -f pnpm-lock.yaml; pnpm i",
    "pretty:code": "prettier --write *.ts",
    "pretty:project": "prettier --write .",
    "test:local": "pnpm run package; pnpm run start:clean --prefix ./local-test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/4lch4/Toggl-Aid"
  },
  "keywords": [
    "toggl",
    "toggl-api",
    "toggl-cli",
    "toggl-report",
    "toggle-reports",
    "wrapper",
    "wrapper-api",
    "wrapper-library"
  ],
  "author": {
    "email": "hello@4lch4.dev",
    "name": "4lch4",
    "url": "https://4lch4.dev"
  },
  "license": "BSD-2-Clause",
  "dependencies": {
    "@4lch4/axios-aid": "^1.2.2"
  },
  "devDependencies": {
    "prettier": "2.0.5",
    "typescript": "3.9.5"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid",
    "semi": false,
    "tabWidth": 2,
    "useTabs": false,
    "trailingComma": "none"
  }
}
